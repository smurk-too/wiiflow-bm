+s32 CMenu::get_title_dir(u32 dirhi, dirent_t *ent, u32 *cnt)
+{
+	s32 res;
+	u32 num = 0;
+	char path[ISFS_MAXPATH + 4];
+
+	if( NULL==cnt )
+		return -9;
+
+	snprintf(path, sizeof(path), "/title/%08x", dirhi);
+	res = ISFS_ReadDir(path, NULL, &num);
+	if(res != ISFS_OK)
+	{
+		error(sfmt("Error: ISFS_ReadDir path %s return %d\n", path, res));
+		return -1;
+	}
+
+	if( ent!=NULL && *cnt<num )
+	{
+		error(L"Error: not enough buffer size store return items!\n");
+		return -8;
+	}
+
+	*cnt = num;
+	if( ent==NULL || 0==num )
+		return 0;
+
+	char *nbuf = (char *)memalign(32, (ISFS_MAXPATH + 4) * num);
+
+	if(nbuf == NULL)
+	{
+		error(L"Error: alloc return NULL!\n");
+		return -2;
+	}
+
+	res = ISFS_ReadDir(path, nbuf, &num);
+	if(res != ISFS_OK)
+	{
+		SAFE_FREE(nbuf);
+		error(sfmt("Error: ISFS_ReadDir return %d\n", res));
+		return -3;
+	}
+	
+	char * p = nbuf;
+	for(u32 i = 0; i < num; i++)
+	{	    
+		memset(&(ent[i]), 0, sizeof(dirent_t));
+		strcpy(ent[i].dirname, p);
+		ent[i].id_hi = dirhi;
+		ent[i].id_lo = (u32) strtol(p, NULL, 16);
+		memcpy(ent[i].titleid, &(ent[i].id_lo), sizeof(ent[i].id_lo));
+		strupr(ent[i].titleid);
+		p += strlen(p) + 1;
+	}
+
+	SAFE_FREE(nbuf);
+	return 0;
+}
+
+s32 CMenu::get_apps(u32 dirhi, const char * tlo, dirent_t *ent, u32 *cnt)
+{
+	s32 res;
+	u32 num = 0;
+	char path[ISFS_MAXPATH];
+
+	if( NULL==cnt || NULL==tlo || 0==dirhi )
+		return -9;
+
+	snprintf(path, sizeof(path), "/title/%08x/%s/content", dirhi, tlo);
+	res = ISFS_ReadDir(path, NULL, &num);
+	if(res != ISFS_OK)
+	{
+		error(sfmt("Error: ISFS_ReadDir path %s return %d\n", path, res));
+		return -1;
+	}
+
+	if( ent!=NULL && *cnt<num )
+	{
+		error(L"Error: not enough buffer size store return items!\n");
+		return -8;
+	}
+
+	*cnt = num;
+	if( ent==NULL || 0==num )
+		return 0;
+
+	char *nbuf = (char *)memalign(32, ISFS_MAXPATH * num);
+
+	if(nbuf == NULL)
+	{
+		error(L"Error: alloc return NULL!\n");
+		return -2;
+	}
+
+	res = ISFS_ReadDir(path, nbuf, &num);
+	if(res != ISFS_OK)
+	{
+		SAFE_FREE(nbuf);
+		error(sfmt("Error: ISFS_ReadDir return %d\n", res));
+		return -3;
+	}
+	char * p = nbuf;
+	u32 c = 0;
+	for(u32 i = 0; i < num; i++)
+	{	    
+		if( strcasestr(p, ".APP") )
+		{
+			memset(&(ent[c]), 0, sizeof(dirent_t));
+			strncpy(ent[c].dirname, p, ISFS_MAXPATH);
+			ent[c].id_hi = dirhi;
+			c++;
+		}
+		p += strlen(p) + 1;
+	}
+
+	*cnt = c;
+	SAFE_FREE(nbuf);
+	return 0;
+}
