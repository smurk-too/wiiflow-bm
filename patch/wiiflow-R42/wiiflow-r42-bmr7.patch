+u32 _btnRepeat(u32 btn, s32 ps)
+{
+	static u32 lastbtn = 0;
+	static u64 delaybtn = 0;
+	static u64 timeoutbtn = 0;
+	u64 nowclk = ticks_to_millisecs(gettime());
+	const u64 cdelay = 180; //0.18sec
+	const u64 ctimout = 650; //0.65sec
+
+	if( !(btn & (WPAD_BUTTON_LEFT | WPAD_BUTTON_RIGHT)) && 
+		!(ps & (WPAD_BUTTON_LEFT | WPAD_BUTTON_RIGHT)) )
+		return 0;
+
+	u32 b=0;
+	
+	if( btn & WPAD_BUTTON_LEFT )
+	{
+		if( (nowclk>delaybtn) || (lastbtn!=WPAD_BUTTON_LEFT) )
+		{
+			b |= WPAD_BUTTON_LEFT;
+			if( nowclk>timeoutbtn || (lastbtn!=WPAD_BUTTON_LEFT) )
+			{
+				delaybtn = nowclk + ctimout;
+				timeoutbtn = delaybtn + cdelay;
+			}
+			else
+			{
+				delaybtn = nowclk + cdelay;
+				timeoutbtn = delaybtn + ctimout;
+			}
+		}
+		lastbtn = WPAD_BUTTON_LEFT;
+	}
+	else if( btn & WPAD_BUTTON_RIGHT )
+	{
+		if( (nowclk>delaybtn) || (lastbtn!=WPAD_BUTTON_RIGHT) )
+		{
+			b |= WPAD_BUTTON_RIGHT;
+			if( nowclk>timeoutbtn || (lastbtn!=WPAD_BUTTON_RIGHT) )
+			{
+				delaybtn = nowclk + ctimout;
+				timeoutbtn = delaybtn + cdelay;
+			}
+			else
+			{
+				delaybtn = nowclk + cdelay;
+				timeoutbtn = delaybtn + ctimout;
+			}
+		}
+		lastbtn = WPAD_BUTTON_RIGHT;
+	}
+	if( ps & WPAD_BUTTON_LEFT )
+		return WPAD_BUTTON_LEFT;
+	else if( ps & WPAD_BUTTON_RIGHT )
+		return WPAD_BUTTON_RIGHT;
+	return b;
+}
+
+bool more_input_scan(s32 & ps, u32 & btn)
+{
+	static unordered_map<u16,s32> kb2wpad;
+	static unordered_map<s32,s32> cpad2wpad;
+	static unordered_map<s32,s32> pad2wpad;
+	if( 0==kb2wpad.size() )
+	{
+		kb2wpad[KS_Escape] = WPAD_BUTTON_HOME;
+		kb2wpad[KS_Home] = WPAD_BUTTON_HOME;
+		kb2wpad[KS_Right] = WPAD_BUTTON_RIGHT;
+		kb2wpad[KS_Left] = WPAD_BUTTON_LEFT;
+		kb2wpad[KS_Up] = WPAD_BUTTON_UP;
+		kb2wpad[KS_Down] = WPAD_BUTTON_DOWN;
+		kb2wpad[KS_plus] = WPAD_BUTTON_PLUS;
+		kb2wpad[KS_equal] = WPAD_BUTTON_PLUS;
+		kb2wpad[KS_minus] = WPAD_BUTTON_MINUS;
+		kb2wpad[KS_underscore] = WPAD_BUTTON_MINUS;
+		kb2wpad[KS_Return] = WPAD_BUTTON_A;
+		kb2wpad[KS_A] = WPAD_BUTTON_A;
+		kb2wpad[KS_a] = WPAD_BUTTON_A;
+		kb2wpad[KS_BackSpace] = WPAD_BUTTON_B;
+		kb2wpad[KS_B] = WPAD_BUTTON_B;
+		kb2wpad[KS_b] = WPAD_BUTTON_B;
+		kb2wpad[KS_1] = WPAD_BUTTON_1;
+		kb2wpad[KS_2] = WPAD_BUTTON_2;
+
+		kb2wpad[KS_KP_Home] = WPAD_BUTTON_HOME;
+		kb2wpad[KS_KP_Left] = WPAD_BUTTON_LEFT;
+		kb2wpad[KS_KP_Right] = WPAD_BUTTON_RIGHT;
+		kb2wpad[KS_KP_Up] = WPAD_BUTTON_UP;
+		kb2wpad[KS_KP_Down] = WPAD_BUTTON_DOWN;
+		kb2wpad[KS_KP_1] = WPAD_BUTTON_1;
+		kb2wpad[KS_KP_End] = WPAD_BUTTON_1;
+		kb2wpad[KS_KP_2] = WPAD_BUTTON_2;
+		kb2wpad[KS_KP_Enter] = WPAD_BUTTON_A;
+		kb2wpad[KS_KP_Insert] = WPAD_BUTTON_B;
+		kb2wpad[KS_KP_Add] = WPAD_BUTTON_PLUS;
+		kb2wpad[KS_KP_Subtract] = WPAD_BUTTON_MINUS;
+
+		cpad2wpad[WPAD_CLASSIC_BUTTON_HOME] = WPAD_BUTTON_HOME;
+		cpad2wpad[WPAD_CLASSIC_BUTTON_UP] = WPAD_BUTTON_UP;
+		cpad2wpad[WPAD_CLASSIC_BUTTON_LEFT] = WPAD_BUTTON_LEFT;
+		cpad2wpad[WPAD_CLASSIC_BUTTON_DOWN] = WPAD_BUTTON_DOWN;
+		cpad2wpad[WPAD_CLASSIC_BUTTON_RIGHT] = WPAD_BUTTON_RIGHT;
+		cpad2wpad[WPAD_CLASSIC_BUTTON_A] = WPAD_BUTTON_A;
+		cpad2wpad[WPAD_CLASSIC_BUTTON_B] = WPAD_BUTTON_B;
+		cpad2wpad[WPAD_CLASSIC_BUTTON_X] = WPAD_BUTTON_1;
+		cpad2wpad[WPAD_CLASSIC_BUTTON_Y] = WPAD_BUTTON_2;
+		cpad2wpad[WPAD_CLASSIC_BUTTON_PLUS] = WPAD_BUTTON_PLUS;
+		cpad2wpad[WPAD_CLASSIC_BUTTON_MINUS] = WPAD_BUTTON_MINUS;
+
+		pad2wpad[PAD_TRIGGER_Z] = WPAD_BUTTON_HOME;
+		pad2wpad[PAD_BUTTON_UP] = WPAD_BUTTON_UP;
+		pad2wpad[PAD_BUTTON_LEFT] = WPAD_BUTTON_LEFT;
+		pad2wpad[PAD_BUTTON_DOWN] = WPAD_BUTTON_DOWN;
+		pad2wpad[PAD_BUTTON_RIGHT] = WPAD_BUTTON_RIGHT;
+		pad2wpad[PAD_BUTTON_A] = WPAD_BUTTON_A;
+		pad2wpad[PAD_BUTTON_B] = WPAD_BUTTON_B;
+		pad2wpad[PAD_BUTTON_X] = WPAD_BUTTON_1;
+		pad2wpad[PAD_BUTTON_Y] = WPAD_BUTTON_2;
+		pad2wpad[PAD_TRIGGER_R] = WPAD_BUTTON_PLUS;
+		pad2wpad[PAD_TRIGGER_L] = WPAD_BUTTON_MINUS;
+	}
+
+	bool ret = false;
+	btn = 0;
+	
+	// Classic pad
+	const s32 cpadarr[] = {WPAD_CLASSIC_BUTTON_HOME, WPAD_CLASSIC_BUTTON_UP, 
+	WPAD_CLASSIC_BUTTON_LEFT, WPAD_CLASSIC_BUTTON_DOWN, WPAD_CLASSIC_BUTTON_RIGHT,
+	WPAD_CLASSIC_BUTTON_A, WPAD_CLASSIC_BUTTON_B, WPAD_CLASSIC_BUTTON_X, 
+	WPAD_CLASSIC_BUTTON_Y, WPAD_CLASSIC_BUTTON_PLUS, WPAD_CLASSIC_BUTTON_MINUS};
+	unordered_map<s32,s32>::const_iterator ci;
+	for( u32 i = 0; i<sizeof(cpadarr)/sizeof(cpadarr[0]); ++i)
+	{
+		if( ps & cpadarr[i] )
+		{
+			ci = cpad2wpad.find(cpadarr[i]);
+			if( cpad2wpad.end() != ci )
+			{
+				ps &= ~cpadarr[i];
+				ps |= ci->second;
+				ret = true;
+			}
+		}
+	}
+	u32 wpadhs = WPAD_ButtonsHeld(0);
+	u32 btnin = 0;
+	if( wpadhs & (WPAD_CLASSIC_BUTTON_LEFT|WPAD_BUTTON_LEFT) )
+		btnin |= WPAD_BUTTON_LEFT;
+	if( wpadhs & (WPAD_CLASSIC_BUTTON_RIGHT|WPAD_BUTTON_RIGHT) )
+		btnin |= WPAD_BUTTON_RIGHT;
+	if( btnin )
+	{
+		btn = _btnRepeat(btnin, ps);
+		if( btn )
+			ret = true;
+	}
+	if( ret )
+		return true;
+
+	// NGC pad
+	const s32 padarr[] = {PAD_TRIGGER_Z, PAD_BUTTON_UP, PAD_BUTTON_LEFT, PAD_BUTTON_DOWN, PAD_BUTTON_RIGHT,
+	PAD_BUTTON_A, PAD_BUTTON_B, PAD_BUTTON_X, PAD_BUTTON_Y, PAD_TRIGGER_R, PAD_TRIGGER_L};
+	PAD_ScanPads();
+	s32 padps = PAD_ButtonsDown(PAD_CHAN0);
+	s32 padout = 0;
+	unordered_map<s32,s32>::const_iterator pi;
+	for( u32 i = 0; i<sizeof(padarr)/sizeof(padarr[0]); ++i)
+	{
+		if( padps & padarr[i] )
+		{
+			pi = pad2wpad.find( padarr[i] );
+			if( pad2wpad.end() != pi )
+			{
+				padout |= pi->second;
+				ret = true;
+			}
+		}
+	}
+	btnin = 0;
+	s32 padhs = PAD_ButtonsHeld(PAD_CHAN0);
+	if( padhs & PAD_BUTTON_LEFT )
+		btnin |= WPAD_BUTTON_LEFT;
+	if( padhs & PAD_BUTTON_RIGHT )
+		btnin |= WPAD_BUTTON_RIGHT;
+	if( btnin )
+	{
+		btn = _btnRepeat(btnin, padout);
+		if( btn )
+			ret = true;
+	}
+	if( ret )
+	{
+		ps = padout;
+		return true;
+	}
+	
+	// Keyboard
+	keyboard_event ke;
+	s32 kst = KEYBOARD_GetEvent(&ke);
+	if( 0==kst || ke.type != KEYBOARD_PRESSED )
+		return false;
+	unordered_map<u16,s32>::const_iterator ki;
+	ki = kb2wpad.find(ke.symbol);
+	if( kb2wpad.end() == ki )
+		return false;
+	ps = ki->second;
+	btnin = 0;
+	if( KS_KP_Left==ke.symbol || KS_Left==ke.symbol )
+		btnin |= WPAD_BUTTON_LEFT;
+	if( KS_KP_Right==ke.symbol || KS_Right==ke.symbol )
+		btnin |= WPAD_BUTTON_RIGHT;
+	if( btnin )
+		btn = _btnRepeat(btnin, btnin);
+	if( !btn && !ps )
+		return false;
+	return true;
+}
+
+static int exit_action = HKM_NOTHING;
+static u64 gp_tid = 0;
+
+bool CMenu::_home_key_menu(void)
+{
+	s32 padsState;
+	WPADData *wd;
+	u32 btn = 0;
+	bool ret = false;
+
+	WPAD_Rumble(WPAD_CHAN_0, 0);
+	_showHomeKeyMenu();
+	do
+	{
+		WPAD_ScanPads();
+		padsState = WPAD_ButtonsDown(0);
+		wd = WPAD_Data(0);
+		more_input_scan(padsState, btn);
+		if( padsState & WPAD_BUTTON_RIGHT ) {
+				exit_action = HKM_REBOOT;
+				ret = true;
+				break;
+		} else if( padsState & WPAD_BUTTON_LEFT ) {
+				exit_action = HKM_IDLE;
+				ret = true;
+				break;
+		} else if( padsState & WPAD_BUTTON_UP ) {
+				char code[4];
+				_hideHomeKeyMenu();
+				if (_code(code) && memcmp(code, m_cfg.getString(" GENERAL", "parent_code", "").c_str(), 4) == 0)
+					m_locked = false;
+				_showHomeKeyMenu();
+				ret = false;
+				break;
+		} else if( padsState & WPAD_BUTTON_DOWN ) {
+				exit_action = HKM_SHUTDOWN;
+				ret = true;
+				break;
+		} else if( padsState & WPAD_BUTTON_PLUS ) {
+				ret = _run_channel_menu();
+				break;;
+		} else if( padsState & (WPAD_BUTTON_HOME | WPAD_BUTTON_B) ) {
+				ret = false;
+				break;;
+		} else if( !m_locked && (padsState & WPAD_BUTTON_MINUS) ) {
+				*(vu32*)0x8132FFFB = 0x50756e65;
+				DCFlushRange((void*)0x8132FFFB,4);
+				exit_action = HKM_REBOOT;
+				ret = true;
+				break;
+		} else if( !m_locked && (padsState & WPAD_BUTTON_1) ) {
+				exit_action = HKM_HBC;
+				ret = true;
+				break;
+		} else if( !m_locked && (padsState & WPAD_BUTTON_2) ) {
+				*(vu32*)0x8132FFFB = 0x4461636f;
+				DCFlushRange((void*)0x8132FFFB,4);
+				exit_action = HKM_REBOOT;
+				ret = true;
+				break;
+		} else if( padsState & WPAD_BUTTON_A ) {
+				ret = true;
+				break;
+		}
+		_mainLoopCommon(wd);
+	} while (true);
+	WPAD_Rumble(WPAD_CHAN_0, 0);
+	_hideHomeKeyMenu(false);
+	return ret;
+}
