+{
+	if( titles.loaded() )
+	{
+		string phrname(titles.getString("TITLES", t->titleid));
+		if( !phrname.empty() )
+		{
+			strncpy(t->hrname, phrname.c_str(), 64/* TITLE_MAX */);
+			return;
+		}
+	}
+
+	s32 ret;
+	char path[ISFS_MAXPATH];
+	u8 hrname[HR_NAMELEN];
+	u8 * buf = (u8 *) memalign(32, HR_BUFF_LEN+16);
+	dirent_t * applst = (dirent_t *) memalign(32, sizeof(dirent_t) * (number+4));
+
+	if( buf==NULL || NULL==applst )
+	{
+		error(L"Error: alloc buf return NULL!\n");
+		return;
+	}
+
+	memset(buf, 0, HR_BUFF_BLEN);
+	snprintf(path, sizeof(path), "/title/%08x/%s/data/banner.bin", t->id_hi, t->dirname);
+	s32 cfd = ISFS_Open(path, ISFS_OPEN_READ);
+	if( cfd>=0 )
+	{
+		ret = ISFS_Read(cfd, buf, HR_BUFF_BLEN);
+		usleep(1000);
+		ISFS_Close(cfd);
+		if( ret > (s32) 0x21 )
+		{
+			u32 idx = 0; 
+			s32 loc;
+			memset(hrname, 0, sizeof(hrname));
+			for( loc = 0x21; loc < ret && loc < HR_BUFF_BLEN && buf[loc] && idx<254; loc +=2 )
+				hrname[idx++] = buf[loc];
+			if( idx )
+			{
+				strncpy(t->hrname, (char*)hrname, sizeof(hrname));
+				SAFE_FREE(applst);
+				SAFE_FREE(buf);
+				return;
+			}
+		}
+		else
+		{
+			error(sfmt("Error: ISFS_Read return %d\n", ret));
+		}
+	}
+
+	u32 capp = number;
+	ret = get_apps(t->id_hi, t->dirname, applst, &capp);
+
+	if( 0==ret && capp>0 )
+	{
+		u32 j;
+		for( j=0; j<capp; j++ )
+		{
+			memset(buf, 0, HR_BUFF_LEN);
+			snprintf(path, sizeof(path), "/title/%08x/%s/content/%s", t->id_hi, t->dirname, applst[j].dirname);
+			s32 cfd2 = ISFS_Open(path, ISFS_OPEN_READ);
+			if( cfd2>=0 )
+			{
+				ret = ISFS_Read(cfd2, buf, HR_BUFF_LEN);
+				usleep(1000);
+				ISFS_Close(cfd2);
+				if( ret>241 )
+				{
+					if( 0==memcmp(&(buf[0x80]), "IMET", 4) )
+					{
+						u32 idx = 0; 
+						s32 loc;
+						memset(hrname, 0, sizeof(hrname));
+						for( loc=0xf1; loc<ret && loc<HR_BUFF_LEN && buf[loc] && idx<254; loc +=2 )
+							hrname[idx++] = buf[loc];
+						if( idx )
+						{
+							strncpy(t->hrname, (char*)hrname, sizeof(hrname));
+							SAFE_FREE(applst);
+							SAFE_FREE(buf);
+							return;
+						}
+					}
+				}
+			}
+			else error(sfmt("Error: ISFS_Open %s.\n", path));
+		}
+	}
+	else 
+		error(sfmt("Error: get_apps get count %d %u failed.", ret, capp));
+	SAFE_FREE(applst);
+	SAFE_FREE(buf);
+}
