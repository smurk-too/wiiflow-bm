+u64 CMenu::show_channel_menu(channel_list_t & chlist)
+{
+	// init page line string
+	wchar_t linestr[9][HR_NAMELEN];
+	wcsncpy(linestr[0], _t("chmline1", L"U: [%s] %s\n").c_str(), HR_NAMELEN);
+	wcsncpy(linestr[1], _t("chmline2", L"D: [%s] %s\n").c_str(), HR_NAMELEN);
+	wcsncpy(linestr[2], _t("chmline3", L"L: [%s] %s\n").c_str(), HR_NAMELEN);
+	wcsncpy(linestr[3], _t("chmline4", L"R: [%s] %s\n").c_str(), HR_NAMELEN);
+	wcsncpy(linestr[4], _t("chmline5", L"A: [%s] %s\n").c_str(), HR_NAMELEN);
+	wcsncpy(linestr[5], _t("chmline6", L"B: [%s] %s\n").c_str(), HR_NAMELEN);
+	wcsncpy(linestr[6], _t("chmline7", L"1: [%s] %s\n").c_str(), HR_NAMELEN);
+	wcsncpy(linestr[7], _t("chmline8", L"2: [%s] %s\n").c_str(), HR_NAMELEN);
+	wcsncpy(linestr[8], _t("chmline9", L"-: [%s] %s\n").c_str(), HR_NAMELEN);
+	const u32 dfbtn[11] = { WPAD_BUTTON_UP, WPAD_BUTTON_DOWN, WPAD_BUTTON_LEFT, WPAD_BUTTON_RIGHT,
+		WPAD_BUTTON_A, WPAD_BUTTON_B, WPAD_BUTTON_1, WPAD_BUTTON_2,
+		WPAD_BUTTON_MINUS, WPAD_BUTTON_PLUS, WPAD_BUTTON_HOME };
+
+	u32 cpage=0, i;
+	const u32 cnt = (u32) chlist.size();
+	const u32 mpage=cnt/9 + 1;
+	wstringEx txtbox;
+	bool drawtxt = true;
+	bool quit = false;
+	u64 ret64 = 0;
+
+	WPAD_Rumble(WPAD_CHAN_0, 0);
+	_setBg(m_homeBg, m_homeBg);
+//	_textHomeKeyMenu();
+	m_btnMgr.setText(m_homeLblTitle, _t("launchchannel", L"Launch Channel"), true);
+	m_btnMgr.setText(m_homeLblTextBox, L"", true);
+	m_btnMgr.show(m_homeLblTitle);
+	m_btnMgr.show(m_homeLblTextBox);
+	
+	do
+	{
+		//show menu
+		if( drawtxt )
+		{
+			drawtxt = false;
+			u32 cur = cpage*9;
+			txtbox = _t("chmlinea",L"Home: exit\n\n");
+			for( i=0; i<9 && cur<cnt; ++i, ++cur )
+			{
+				txtbox += wfmt(linestr[i], chlist[cur].titleid, chlist[cur].hrname );
+			}
+			txtbox += L"\n\n";
+			txtbox += wfmt(_t("chmlineb", L"+: next page      Page: %u / %u\n"), cpage+1, mpage);
+			m_btnMgr.setText(m_homeLblTextBox, txtbox, true);
+		}
+
+		WPAD_ScanPads();
+		s32 padsState = WPAD_ButtonsDown(0);
+		WPADData * wd = WPAD_Data(0);
+		s32 kbstat=0;
+		if( kb_scan(kbstat) )
+			padsState = kbstat;
+		for( i=0; i<11 && padsState; ++i )
+			if( padsState & dfbtn[i] )
+			{
+				if( dfbtn[i]==WPAD_BUTTON_PLUS )
+				{
+					cpage = (cpage + 1) % mpage;
+					drawtxt = true;
+					break;
+				}
+				else if( dfbtn[i]==WPAD_BUTTON_HOME )
+				{
+					quit = true;
+					break;
+				}
+				const u32 cloc = cpage*9+i;
+				if( cloc < cnt )
+				{
+					ret64 = (u64)(chlist[cloc].id_hi)<<32 | chlist[cloc].id_lo;
+					quit = true;
+					break;
+				}
+			}
+		_mainLoopCommon(wd);
+	}while( false==quit );
+	WPAD_Rumble(WPAD_CHAN_0, 0);
+	_hideHomeKeyMenu(false);
+	return ret64;
+}
+
+bool CMenu::_run_channel_menu(void)
+{
+	u64 tid = 0;
+	channel_list_t chlist;
+
+	ISFS_Initialize();
+	get_channel_list(chlist);
+	ISFS_Deinitialize();
+
+	if( chlist.size() )
+	{
+		filter_channel_list(chlist);
+		if( chlist.size() )
+		{
+			tid = show_channel_menu(chlist);
+		}
+		else
+		{
+			error(_t("channelerr",L"Error: Channel list is empty. Press any key continue...\n"));
+		}
+	}
+
+	if( tid )
+	{
+		//run_title(tid);
+		gp_tid = tid;
+		exit_action = HKM_RUN_TITLE;
+		return true;
+	}
+	return false;
+}
+
+void Wpad_off(void)
+{
+	for(u32 i=0; i<4; ++i)
+		WPAD_Disconnect(i);
+	WPAD_Shutdown();
+}
+
+void run_title(void)
+{
+	u64 tid = gp_tid;
+	if( 0==tid )
+		return;
+	WII_Initialize();
+	usleep(1000);
+	s32 ret = WII_LaunchTitle( tid );
+	if( ret<0 )
+		return;
+	exit(0);
+}
+
+void Exit_task(void)
+{
+	Wpad_off();
+//	KEYBOARD_Deinit();
+	switch(exit_action)
+	{
+	case HKM_REBOOT:
+		Sys_Reboot();
+		break;
+	case HKM_IDLE:
+		Sys_Shutdown();
+		break;
+	case HKM_SHUTDOWN:
+		STM_ShutdownToStandby();
+		break;
+	case HKM_HBC:
+		WII_Initialize();
+	    WII_LaunchTitle(0x000100014A4F4449LL); // JODI
+	    WII_LaunchTitle(0x0001000148415858LL); // HAXX
+		break;
+	case HKM_RUN_TITLE:
+		run_title();
+		break;
+	case HKM_NOTHING:
+	default:
+		break;
+	}
+}
