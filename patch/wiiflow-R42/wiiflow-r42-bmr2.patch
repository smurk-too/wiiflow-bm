+s32 CMenu::get_channel_list(channel_list_t & chlist)
+{
+	s32 ret;
+	u32 maxnum1 = 0;
+	u32 maxnum2 = 0;
+	chlist.clear();
+
+	ret = get_title_dir(0x00010001, NULL, &maxnum1);
+	if( 0!=ret )
+	{
+		error(L"Error: unable to open dir1 or no file in dir.\n");
+		return ret;
+	}
+	ret = get_title_dir(0x00010002, NULL, &maxnum2);
+	if( 0!=ret )
+	{
+		error(L"Error: unable to open dir2 or no file in dir.\n");
+		return ret;
+	}
+
+	u32 tmpnum = maxnum1+maxnum2;
+	if( 0==tmpnum )
+	{
+		error(L"Error: 0 item!\n");
+		return -7;
+	}
+	
+	dirent_t * tmplist = (dirent_t *) memalign(32, sizeof(dirent_t) * tmpnum);
+	if( NULL==tmplist )
+	{
+		error(L"Error: alloc return NULL!\n");
+		return -2;
+	}
+	memset(tmplist, 0, sizeof(dirent_t) * tmpnum);
+
+	if( maxnum1>0 )
+	{
+		ret = get_title_dir(0x00010001, tmplist, &maxnum1);
+		if( ret != 0)
+		{
+			SAFE_FREE(tmplist);
+			error(L"Error Reading folder dir1 failed.\n");
+			return ret;
+		}
+	}
+	if( maxnum2>0 )
+	{
+		ret = get_title_dir(0x00010002, &(tmplist[maxnum1]), &maxnum2);
+		if( ret != 0)
+		{
+			SAFE_FREE(tmplist);
+			error(L"Error Reading folder dir2 failed.\n");
+			return ret;
+		}
+	}
+
+	u32 number, i;
+    char path[ISFS_MAXPATH];
+	Config titles;
+	titles.load(sfmt("%s/titles.ini", m_dataDir.c_str()).c_str());	
+
+	for( i = 0; i < tmpnum; i++ )
+	{	
+		dirent_t * t = &(tmplist[i]);
+		snprintf(path, sizeof(path), "/title/%08x/%s/content", t->id_hi, t->dirname);
+		number = 0;
+		ret = ISFS_ReadDir(path, NULL, &number);	
+		if( ISFS_OK==ret && number>1 )
+		{
+			get_hrname(t, number, titles);
+//			printf("%x [%s]%s\n", t->id_hi, t->titleid, t->hrname);
+//			error(sfmt("%x [%s]%s\n", t->id_hi, t->titleid, t->hrname));
+			chlist.push_back( *t );
+		}
+	}
+
+	SAFE_FREE(tmplist);
+	return 0;
+}
+
+void CMenu::filter_channel_list(channel_list_t & chlist)
+{
+	Config channels;
+	channels.load(sfmt("%s/channels.ini", m_dataDir.c_str()).c_str());	
+	if( !channels.loaded() )
+		return;
+again:
+	channel_list_t::iterator ich = chlist.begin();
+	for( ; ich != chlist.end(); ich++ )
+	{
+		int v = channels.getInt("CHANNELS", ich->titleid, 99);
+		if( 0==v || ( m_locked && 1==v ) )
+		{
+			chlist.erase( ich );
+			goto again;
+		}
+	}
+}
