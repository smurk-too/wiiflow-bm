+static int exit_action = HKM_NOTHING;
+static u64 gp_tid = 0;
+
+bool CMenu::_home_key_menu(void)
+{
+	s32 padsState;
+	WPADData *wd;
+	bool ret = false;
+
+	WPAD_Rumble(WPAD_CHAN_0, 0);
+	_showHomeKeyMenu();
+	do
+	{
+		WPAD_ScanPads();
+		padsState = WPAD_ButtonsDown(0);
+		wd = WPAD_Data(0);
+		s32 kbstat=0;
+		if( kb_scan(kbstat) )
+			padsState = kbstat;
+		if( padsState & WPAD_BUTTON_RIGHT ) {
+				exit_action = HKM_REBOOT;
+				ret = true;
+				break;
+		} else if( padsState & WPAD_BUTTON_LEFT ) {
+				exit_action = HKM_IDLE;
+				ret = true;
+				break;
+		} else if( padsState & WPAD_BUTTON_UP ) {
+				char code[4];
+				_hideHomeKeyMenu();
+				if (_code(code) && memcmp(code, m_cfg.getString(" GENERAL", "parent_code", "").c_str(), 4) == 0)
+					m_locked = false;
+				_showHomeKeyMenu();
+				ret = false;
+				break;
+		} else if( padsState & WPAD_BUTTON_DOWN ) {
+				exit_action = HKM_SHUTDOWN;
+				ret = true;
+				break;
+		} else if( padsState & WPAD_BUTTON_PLUS ) {
+				ret = _run_channel_menu();
+				break;;
+		} else if( padsState & (WPAD_BUTTON_HOME | WPAD_BUTTON_B) ) {
+				ret = false;
+				break;;
+		} else if( !m_locked && (padsState & WPAD_BUTTON_MINUS) ) {
+				*(vu32*)0x8132FFFB = 0x50756e65;
+				DCFlushRange((void*)0x8132FFFB,4);
+				exit_action = HKM_REBOOT;
+				ret = true;
+				break;
+		} else if( !m_locked && (padsState & WPAD_BUTTON_1) ) {
+				exit_action = HKM_HBC;
+				ret = true;
+				break;
+		} else if( !m_locked && (padsState & WPAD_BUTTON_2) ) {
+				*(vu32*)0x8132FFFB = 0x4461636f;
+				DCFlushRange((void*)0x8132FFFB,4);
+				exit_action = HKM_REBOOT;
+				ret = true;
+				break;
+		} else if( padsState & WPAD_BUTTON_A ) {
+				ret = true;
+				break;
+		}
+		_mainLoopCommon(wd);
+	} while (true);
+	WPAD_Rumble(WPAD_CHAN_0, 0);
+	_hideHomeKeyMenu(false);
+	return ret;
+}
