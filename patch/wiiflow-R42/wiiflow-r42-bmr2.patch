+bool kb_scan(s32 & kbstate)
+{
+	kbstate = 0;
+	keyboard_event ke;
+	s32 kst = KEYBOARD_GetEvent(&ke);
+	if( 0==kst || ke.type != KEYBOARD_PRESSED )
+		return false;
+	static unordered_map<u16,s32> kb2wpad;
+	if( 0==kb2wpad.size() )
+	{
+		kb2wpad[KS_Escape] = WPAD_BUTTON_HOME;
+		kb2wpad[KS_Home] = WPAD_BUTTON_HOME;
+		kb2wpad[KS_Right] = WPAD_BUTTON_RIGHT;
+		kb2wpad[KS_Left] = WPAD_BUTTON_LEFT;
+		kb2wpad[KS_Up] = WPAD_BUTTON_UP;
+		kb2wpad[KS_Down] = WPAD_BUTTON_DOWN;
+		kb2wpad[KS_plus] = WPAD_BUTTON_PLUS;
+		kb2wpad[KS_equal] = WPAD_BUTTON_PLUS;
+		kb2wpad[KS_minus] = WPAD_BUTTON_MINUS;
+		kb2wpad[KS_underscore] = WPAD_BUTTON_MINUS;
+		kb2wpad[KS_Return] = WPAD_BUTTON_A;
+		kb2wpad[KS_A] = WPAD_BUTTON_A;
+		kb2wpad[KS_a] = WPAD_BUTTON_A;
+		kb2wpad[KS_BackSpace] = WPAD_BUTTON_B;
+		kb2wpad[KS_B] = WPAD_BUTTON_B;
+		kb2wpad[KS_b] = WPAD_BUTTON_B;
+		kb2wpad[KS_1] = WPAD_BUTTON_1;
+		kb2wpad[KS_2] = WPAD_BUTTON_2;
+
+		kb2wpad[KS_KP_Home] = WPAD_BUTTON_HOME;
+		kb2wpad[KS_KP_Left] = WPAD_BUTTON_LEFT;
+		kb2wpad[KS_KP_Right] = WPAD_BUTTON_RIGHT;
+		kb2wpad[KS_KP_Up] = WPAD_BUTTON_UP;
+		kb2wpad[KS_KP_Down] = WPAD_BUTTON_DOWN;
+		kb2wpad[KS_KP_1] = WPAD_BUTTON_1;
+		kb2wpad[KS_KP_End] = WPAD_BUTTON_1;
+		kb2wpad[KS_KP_2] = WPAD_BUTTON_2;
+		kb2wpad[KS_KP_Enter] = WPAD_BUTTON_A;
+		kb2wpad[KS_KP_Insert] = WPAD_BUTTON_B;
+		kb2wpad[KS_KP_Add] = WPAD_BUTTON_PLUS;
+		kb2wpad[KS_KP_Subtract] = WPAD_BUTTON_MINUS;
+	}
+	unordered_map<u16,s32>::const_iterator ki;
+	ki = kb2wpad.find(ke.symbol);
+	if( kb2wpad.end() == ki )
+		return false;
+	kbstate = ki->second;
+	return true;
+}
